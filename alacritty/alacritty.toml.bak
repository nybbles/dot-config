# themester_managed = "Theme: Instinct Dark"

[colors.bright]
black = "#2a2a2a"
blue = "#5dade2"
cyan = "#5ddffd"
green = "#50fa7b"
magenta = "#bd79d1"
red = "#ff5555"
white = "#ffffff"
yellow = "#ffff55"

[colors.cursor]
cursor = "#ff6b35"
text = "#0a0a0a"

[colors.footer_bar]
background = "#2a2a2a"
foreground = "#f5f5f5"

[colors.hints.end]
background = "#2a2a2a"
foreground = "#f5f5f5"

[colors.hints.start]
background = "#ffd700"
foreground = "#0a0a0a"

[[colors.indexed_colors]]
color = "#ff6b35"
index = 16

[[colors.indexed_colors]]
color = "#8e44ad"
index = 17

[colors.normal]
black = "#0a0a0a"
blue = "#4a90e2"
cyan = "#17a2b8"
green = "#32cd32"
magenta = "#9b59b6"
red = "#e74c3c"
white = "#f5f5f5"
yellow = "#ffd700"

[colors.primary]
background = "#0a0a0a"
bright_foreground = "#ffffff"
dim_foreground = "#8a8a8a"
foreground = "#f5f5f5"

[colors.search.focused_match]
background = "#32cd32"
foreground = "#0a0a0a"

[colors.search.matches]
background = "#ffd700"
foreground = "#0a0a0a"

[colors.selection]
background = "#ff6b35"
text = "#0a0a0a"

[colors.vi_mode_cursor]
cursor = "#4a90e2"
text = "#0a0a0a"

[env]
TERM = "xterm-256color"

[font]
size = 11.5

[font.normal]
family = "0xProto Nerd Font Mono"
style = "Regular"

[general]
import = ["~/.config/alacritty/catppuccin-latte.toml"]

[[keyboard.bindings]]
action = "ToggleFullscreen"
key = "Return"
mods = "Super"
