# themester_managed = "Theme: Nighthawk Light"

[colors.bright]
black = "#2d3748"
blue = "#3182ce"
cyan = "#319795"
green = "#38a169"
magenta = "#805ad5"
red = "#e53e3e"
white = "#ffffff"
yellow = "#d69e2e"

[colors.cursor]
cursor = "#4a5568"
text = "#f8f9fa"

[colors.footer_bar]
background = "#e2e8f0"
foreground = "#2d3748"

[colors.hints.end]
background = "#e2e8f0"
foreground = "#2d3748"

[colors.hints.start]
background = "#718096"
foreground = "#f8f9fa"

[[colors.indexed_colors]]
color = "#4a5568"
index = 16

[[colors.indexed_colors]]
color = "#2d3748"
index = 17

[colors.normal]
black = "#1a202c"
blue = "#2c5282"
cyan = "#2c7a7b"
green = "#2f855a"
magenta = "#6b46c1"
red = "#c53030"
white = "#e2e8f0"
yellow = "#b7791f"

[colors.primary]
background = "#f8f9fa"
bright_foreground = "#1a202c"
dim_foreground = "#4a5568"
foreground = "#2d3748"

[colors.search.focused_match]
background = "#4a5568"
foreground = "#f8f9fa"

[colors.search.matches]
background = "#718096"
foreground = "#f8f9fa"

[colors.selection]
background = "#4a5568"
text = "#f8f9fa"

[colors.vi_mode_cursor]
cursor = "#2d3748"
text = "#f8f9fa"

[env]
TERM = "xterm-256color"

[font]
size = 11.5

[font.normal]
family = "0xProto Nerd Font Mono"
style = "Regular"

[general]
import = ["~/.config/alacritty/catppuccin-latte.toml"]

[[keyboard.bindings]]
action = "ToggleFullscreen"
key = "Return"
mods = "Super"

[window]
decorations = "none"
