{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\ue285 ",
    "foreground": "#cdd6f4",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "windows": "\ue62a"
          },
          "leading_diamond": "\u256d\u2500\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .HostName }}  ",
          "foreground": "#1e1e2e",
          "background": "#94e2d5",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf292 ",
          "foreground": "#f9e2af",
          "powerline_symbol": "\ue0b0",
          "background": "#f38ba8",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf07c ",
            "folder_separator_icon": "<#1e1e2e>\ue0b1</> ",
            "home_icon": " \ueb06 ",
            "style": "agnoster"
          },
          "template": "{{ path .Path .Location }}",
          "foreground": "#1e1e2e",
          "powerline_symbol": "\ue0b0",
          "background": "#89b4fa",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ url .UpstreamIcon .UpstreamURL }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#1e1e2e",
          "powerline_symbol": "\ue0b0",
          "background": "#a6e3a1",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f9e2af{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fab387{{ end }}",
            "{{ if gt .Ahead 0 }}#cba6f7{{ end }}",
            "{{ if gt .Behind 0 }}#cba6f7{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "leading_diamond": "<transparent,#585b70>\ue0b0</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .FormattedMs }}",
          "foreground": "#cdd6f4",
          "background": "#585b70",
          "type": "executiontime",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "break",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#cdd6f4>\ue0b2</>",
          "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#f38ba8",
          "background": "#cdd6f4",
          "type": "angular",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#45475a>\ue0b2</>",
          "template": "\ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "foreground": "#fab387",
          "background": "#45475a",
          "type": "aws",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "npm_icon": "<#f38ba8>\ue71e</> ",
            "yarn_icon": "<#89b4fa>\ue6a7</> "
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#313244>\ue0b2</>",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#a6e3a1",
          "background": "#313244",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#89b4fa>\ue0b2</>",
          "template": "\ue235  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#f9e2af",
          "background": "#89b4fa",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#cdd6f4>\ue0b2</>",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#89b4fa",
          "background": "#cdd6f4",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#cdd6f4>\ue0b2</>",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#1e1e2e",
          "background": "#cdd6f4",
          "type": "rust",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": "\ue641 {{ .CurrentDate | date .Format }}",
          "foreground": "#cdd6f4",
          "background": "#6c7086",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "#94e2d5",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\ue285\ue285",
          "foreground": "#a6e3a1",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}